ApplicationModules.push (angular) ->
    angular.module("core").config ["$stateProvider", ($stateProvider) ->
        $stateProvider.
            state "home",
                url: "/",
                templateUrl: "<%= asset_path("modules/core/views/home.client.view.html") %>"
        $stateProvider.
            state "about",
                url: "/about",
                templateUrl: "<%= asset_path("modules/core/views/about.client.view.html") %>"
    ]